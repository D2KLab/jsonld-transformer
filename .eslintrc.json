{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-param-reassign": "off",
    "no-console": "warn",
    "no-plusplus": "off",
    "no-continue": "off",
    "no-restricted-syntax": "off",
    "radix": "off",
    "arrow-parens": "off",
    "prefer-object-spread": "off"
  },
  "plugins": [
    "import"
  ]
}
